{
  "name": "nu-lint",
  "displayName": "Nu-Lint",
  "description": "Linter for NuShell",
  "version": "0.0.13",
  "publisher": "WillemVanhulle",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Linters",
    "Programming Languages"
  ],
  "keywords": [
    "nushell",
    "nu",
    "lint",
    "linter",
    "shell"
  ],
  "activationEvents": [
    "onLanguage:nu",
    "onLanguage:nushell"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Nu-Lint",
      "properties": {
        "nu-lint.executablePath": {
          "type": "string",
          "default": "nu-lint",
          "description": "Path to the nu-lint executable"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run build",
    "build": "node esbuild.mjs",
    "watch": "node esbuild.mjs --watch",
    "publish": "vsce publish --no-dependencies"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^3.7.0",
    "esbuild": "^0.24.0",
    "typescript": "^5.0.0"
  },
  "extensionDependencies": [
    "TheNuProjectContributors.vscode-nushell-lang"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wvhulle/nu-lint-vscode.git"
  },
  "icon": "icon.png",
  "license": "MIT"
}